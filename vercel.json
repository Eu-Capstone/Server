{
  "version": 2,
  "routes": [
    { "src": "/api/mbti/result", "dest": "/app.js" },
    { "src": "/api/mbti/count", "dest": "/app.js" },
    { "src": "/(.*)", "dest": "/app.js" }
  ],
  "builds": [
    { "src": "/router/**/*.js", "use": "@now/node" },
    { "src": "/app.js", "use": "@now/node" }
  ]
}
